{
	"name": "@gnosticdev/highlevel-sdk",
	"version": "1.1.5",
	"author": {
		"name": "gnosticdev",
		"url": "https://github.com/gnosticdev"
	},
	"repository": {
		"url": "https://github.com/gnosticdev/highlevel-api-sdk",
		"type": "git"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.2",
		"@types/bun": "^1.1.6",
		"kleur": "^4.1.5",
		"lefthook": "^1.7.17",
		"openapi-typescript": "7.4.1",
		"release-it": "^17.6.0",
		"tsup": "^8.3.0",
		"typescript": "^5.6.3"
	},

	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./config": {
			"default": "./dist/client-config.js",
			"import": "./dist/client-config.js",
			"types": "./dist/client-config.d.ts"
		},
		"./webhooks": {
			"default": "./dist/webhooks.js",
			"import": "./dist/webhooks.js",
			"types": "./dist/webhooks.d.ts"
		},
		"./types/*": {
			"types": "./dist/types/*.d.ts"
		},
		"./oauth": {
			"default": "./dist/oauth.js",
			"import": "./dist/oauth.js",
			"types": "./dist/oauth.d.ts"
		},
		"./oauth-config": {
			"default": "./dist/oauth-config.js",
			"import": "./dist/oauth-config.js",
			"types": "./dist/oauth-config.d.ts"
		},
		"./scopes": {
			"default": "./dist/scopes.js",
			"import": "./dist/scopes.js",
			"types": "./dist/scopes.d.ts"
		}
	},
	"description": "SDK for the HighLevel API",
	"files": ["dist", "README.md", "LICENSE"],
	"homepage": "https://github.com/gnosticdev/highlevel-api-sdk",
	"keywords": [
		"highlevel",
		"api",
		"sdk",
		"typescript",
		"javascript",
		"gohighlevel",
		"crm",
		"marketing"
	],
	"license": "MIT",
	"packageManager": "bun@1.21.1",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build-package": "bun --bun tsup --tsconfig tsconfig.build.json",
		"prepack": "bun test && bun run build-package",
		"generate-api-json": "bun scripts/generate-api-json.ts",
		"generate-api-types": "bun scripts/generate-api-types.ts",
		"generate-webhook-types": "bun scripts/generate-webhook-types.ts",
		"generate-scopes": "bun scripts/generate-scopes.ts",
		"generate-all": "bun run generate-client && bun run generate-api-json && bun run generate-api-types && bun run generate-webhook-types && bun run generate-scopes",
		"lint": "biome check . --write --unsafe",
		"release-it": "release-it",
		"start-test": "bun test --hot"
	},
	"type": "module",
	"peerDependencies": {
		"openapi-fetch": "^0.12.2"
	}
}
