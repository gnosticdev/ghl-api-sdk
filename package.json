{
	"name": "@gnosticdev/highlevel-sdk",
	"version": "1.0.0",
	"repository": "github:gnosticdev/highlevel-api-sdk",
	"main": "dist/client/index.js",
	"devDependencies": {
		"@biomejs/biome": "^1.9.2",
		"@types/bun": "^1.1.6",
		"kleur": "^4.1.5",
		"lefthook": "^1.7.17",
		"openapi-fetch": "^0.12.2",
		"openapi-typescript": "7.4.1",
		"release-it": "^17.6.0",
		"typescript": "^5.5.4"
	},
	"types": "./dist/schema/types/index.d.ts",
	"exports": {
		".": "./dist/client/index.js",
		"./client": "./dist/client/index.js",
		"./types": "./dist/schemas/types/index.js",
		"./scopes": "./dist/client/scopes.js"
	},
	"description": "SDK for the HighLevel API",
	"homepage": "https://github.com/gnosticdev/highlevel-api-sdk",
	"license": "MIT",
	"packageManager": "bun@1.21.1",
	"private": false,
	"scripts": {
		"generate-client": "bun scripts/generate-client.ts",
		"generate-api-json": "bun scripts/generate-api-json.ts",
		"generate-api-types": "bun scripts/generate-api-types.ts",
		"generate-other-types": "bun scripts/generate-other-types.ts",
		"build": "bun run generate-client && bun run generate-api-json && bun run generate-api-types && bun run generate-other-types && bun run package",
		"package": "tsc -p tsconfig.build.json",
		"start-auth": "bun --hot run src/auth/index.tsx",
		"lint": "biome check . --write --unsafe",
		"release": "release-it",
		"start-test": "bun test --hot"
	},
	"type": "module"
}
