{
    "name": "gohighlevel-api",
    "private": true,
    "version": "1.0.0",
    "license": "Apache-2.0",
    "description": "HighLevel API",
    "main": "dist/main.js",
    "exports": {
        ".": "./dist/client/index.js",
        "./types": "./dist/api/types/index.d.ts",
        "./auth": "./dist/api/auth/index.js"
    },
    "type": "module",
    "scripts": {
        "generate:api": "bun scripts/build-api.ts -- generate",
        "generate:client": "bun run scripts/build-client.ts",
        "package": "tsc -p tsconfig.build.json",
        "generate:all": "bun run generate:api && bun run generate:client && bun run package",
        "scrape": "tsc -p playwright/tsconfig.json && node playwright/dist/webhooks/scrape.js"
    },
    "dependencies": {
        "@gnosticdev/cool-console": "link:@gnosticdev/cool-console",
        "crawlee": "^3.6.1",
        "hono": "^3.10.2",
        "nanostores": "^0.9.4",
        "playwright": "^1.40.0",
        "tsx": "^4.1.3",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@apidevtools/swagger-parser": "^10.1.0",
        "@redocly/cli": "latest",
        "@types/js-yaml": "^4.0.9",
        "bun-types": "^1.0.10",
        "js-yaml": "^4.1.0",
        "openapi-fetch": "^0.8.1",
        "openapi-typescript": "7.0.0-next.2",
        "typescript": "^5.2.2"
    }
}
